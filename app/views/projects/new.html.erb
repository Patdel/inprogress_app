<h1>Your New Project</h1>

<%= form_for(@project, class: "form-horizontal") do |f| %>
  <div class="form-group">
  <%= f.label :name %>:
  <%= f.text_field :name %><br />

  <%= f.label :description %>:
  <%= f.text_area :description %><br />

  <%= f.label :picture_url  %>:
  <%= f.text_area :picture_url %><br />

  <%= f.hidden_field :artist_id %>

  <%= f.submit %>
</div>
<% end %>


